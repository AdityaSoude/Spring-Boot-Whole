package com.First.Learn_Spring_framework.game;

public class ContraGame implements GamingConsole {

	public void up() {
		System.out.println("Aim up ");
	}
	public void down() {
		System.out.println("Aim down");
	}
	public void left() {
		System.out.println("fire ");
	}
	@Override
	public void right() {
		System.out.println("sprint");
		
	}
	
}
